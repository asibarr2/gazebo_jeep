<?xml version='1.0'?>

<robot name="myrobot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:property name="cameraSize" value="0.05"/>
  <xacro:property name="cameraMass" value="0.1"/>

  <xacro:include filename="$(find mybot_description)/urdf/mybot.gazebo" />
  <xacro:include filename="$(find mybot_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find mybot_description)/urdf/macros.xacro" />


  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <inertia
          ixx="0.004" ixy="0.001" ixz="0"
          iyy="0.006" iyz="0"
          izz="0.007"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0.36 0.3 0.45" rpy=" 0 0 0"/>
      <geometry>
        <box size="1.3 0.700 0.6"/>
      </geometry>
    </collision> -->

    <visual name='chassis_visual'>
            <!-- DO NOT CHANGE -->
            <origin xyz="0.0 0.0 0.2" rpy=" 1.5707 0.0 0"/>
      <geometry>
        <!-- <box size=".4 .2 .1"/> -->
        <mesh filename="package://mybot_description/meshes/JEEP.dae" scale="1 1 1"/>
      </geometry>
    </visual>
   </link>

  <link name="back_left_wheel">
    <!--origin xyz="0.1 0.13 0.1" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1778" length="0.1524"/>
      </geometry>
    </collision>
    <visual name="left_wheel_visual">
      <origin xyz="0.0 0.0 0.0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <mesh filename="package://mybot_description/meshes/Wheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.1" h="0.05"/>
      <inertia
        ixx="0.004" ixy="0.001" ixz="0.0"
        iyy="0.006" iyz="0.0"
        izz="0.007"/>
    </inertial>
  </link>

  <link name="back_right_wheel">
    <!--origin xyz="0.1 -0.13 0.1" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1778" length="0.1524"/>
      </geometry>
    </collision>
    <visual name="right_wheel_visual">
      <origin xyz="0.0 0.0 0.0" rpy="1.57075 0 0"/>
      <geometry>
        <mesh filename="package://mybot_description/meshes/Wheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <!-- <cylinder_inertia m="5" r="0.1" h="0.05"/> -->
      <inertia
        ixx="0.004" ixy="0.001" ixz="0.0"
        iyy="0.006" iyz="0.0"
        izz="0.007"/>
    </inertial>
  </link>

  <link name="front_right_wheel">
    <!--origin xyz="0.1 -0.13 0.1" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1778" length="0.1524"/>
      </geometry>
    </collision>
    <visual name="right_wheel_visual">
      <origin xyz="0.0 0.0 0.0" rpy="1.57075 0 0"/>
      <geometry>
        <mesh filename="package://mybot_description/meshes/Wheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <!-- <cylinder_inertia m="5" r="0.1" h="0.05"/> -->
      <inertia
        ixx="0.004" ixy="0.001" ixz="0.0"
        iyy="0.006" iyz="0.0"
        izz="0.007"/>
    </inertial>
  </link>

  <link name="front_left_wheel">
    <!--origin xyz="0.1 -0.13 0.1" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1778" length="0.1524"/>
      </geometry>
    </collision>
    <visual name="left_wheel_visual">
      <origin xyz="0.0 0.0 0.0" rpy="1.57075 0 0"/>
      <geometry>
        <mesh filename="package://mybot_description/meshes/Wheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <!-- <cylinder_inertia m="5" r="0.1" h="0.05"/> -->
      <inertia
        ixx="0.004" ixy="0.001" ixz="0.0"
        iyy="0.006" iyz="0.0"
        izz="0.007"/>
    </inertial>
  </link>

  <joint type="continuous" name="back_left_wheel_hinge">
    <origin xyz="0.1 0.8 0" rpy="0 0 0"/>
    <child link="back_left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>

  <joint type="continuous" name="back_right_wheel_hinge">
    <origin xyz="0.1 -0.8 0" rpy="0 0 0"/>
    <child link="back_right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>

  <joint type="continuous" name="front_right_wheel_hinge">
    <origin xyz="0.8 -0.8 0" rpy="0 0 0"/>
    <child link="front_right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
   </joint>

   <joint type="continuous" name="front_left_wheel_hinge">
     <origin xyz="0.8 0.0 0" rpy="0 0 0"/>
     <child link="front_left_wheel"/>
     <parent link="chassis"/>
     <axis xyz="0 1 0" rpy="0 0 0"/>
     <limit effort="100" velocity="100"/>
     <joint_properties damping="0.0" friction="0.0"/>
   </joint>

